[cells]
  root = .
  prelude = prelude
  toolchains = toolchains
  none = none
  buckal = buckal

[cell_aliases]
  config = prelude
  ovr_config = prelude
  fbcode = none
  fbsource = none
  fbcode_macros = none
  buck = none

[external_cells]
  prelude = bundled
  buckal = git

[external_cell_buckal]
  commit_hash = 22bd38c79d2348d9a6591b7156c42d615377eaad
  git_origin = https://github.com/buck2hub/buckal-bundles

[parser]
  target_platform_detector_spec = target:root//...->prelude//platforms:default

[project]
  ignore = .git .buckd .idea .vscode .zed .DS_Store buck-out build .buckal